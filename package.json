{
  "name": "quick-save-forms",
  "version": "1.0.0",
  "description": "Utility script for saving forms fields to localStorage using data- attributes. Makes testing form applications quicker.",
  "main": "qs.js",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jamesrwilliams@github.com/jamesrwilliams/quick-save-forms.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesrwilliams/quick-save-forms/issues"
  },
  "homepage": "https://github.com/jamesrwilliams/quick-save-forms#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  }
}
